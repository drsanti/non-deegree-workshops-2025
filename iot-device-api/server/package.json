{
  "name": "iot-device-api-server",
  "version": "1.0.0",
  "description": "REST API server for IoT device management using Prisma ORM with MongoDB",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
